body {
	background: white;
	color: black;
}

.button {
	width: 100px;
	height: 21px;
}

.header .external-table {
    border-width: 0px;
    border-collapse: collapse;
    border-style: dotted;
}

.header-column {
    border-width: 1px;
    padding: 5px;
    border-style: dotted;
    border-color: '#{a4jSkin.panelBorderColor}';
}

.phases-list {
	font-size: 0.7em;
	height: 130px;
	overflow: visible;
}

a img {
    border: 0px;
}

.valign-top {
    vertical-align: top;
}

ul a.link {
    font-size: 16px;
    font-weight: bold;
}

.log-panel {
    display: none;
}

.width-50 {
	width: 50px;
}

.template-data-grid > tbody > tr > td:first-child {
	width: 50px;
}

.metamer-ftest-class {
	background-color: yellow !important;
	border-width: 5px !important;
	border-color: red !important;
	color: green !important;
	padding: 5px !important;
}